{
  "schemaVersion": 1,
  "id": "${mod_id}",
  "version": "${mod_version}",
  "name": "${mod_name}",
  "description": "${mod_description}",
  "authors": [
    "${mod_authors}"
  ],
  "contact": {
    "homepage": "${mod_page_url}",
    "sources": "${mod_git_url}"
  },
  "license": "${mod_license}",
  "icon": "assets/genesis/icon.png",
  "environment": "*",
  "entrypoints": {
    "main": [
      "dev.mariany.genesis.Genesis"
    ],
    "client": [
      "dev.mariany.genesis.client.GenesisClient"
    ],
    "fabric-datagen": [
      "dev.mariany.genesis.GenesisDataGenerator"
    ],
    "rei_client": [
      "dev.mariany.genesis.compat.rei.client.GenesisClientREIPlugin"
    ],
    "rei_common": [
      "dev.mariany.genesis.compat.rei.GenesisREIPlugin"
    ]
  },
  "mixins": [
    "genesis.mixins.json"
  ],
  "accessWidener": "genesis.accesswidener",
  "depends": {
    "fabricloader": ">=0.16.14",
    "minecraft": ">=1.21.6",
    "java": ">=21",
    "fabric-api": ">=0.128.2",
    "genesisframework": ">=${genesis_framework_version}"
  }
}